all:
  hosts:
    localhost:
      ansible_port: <your port>
      ansible_host: <your ip address>
      root_hash_pass: <the hashed root password as in shadow files>
      ldap_server_location: <ip or domain name of the ldap server>
      ldap_server_netmask: <CIDR mask of the server network>
      radius_client_secret: <secret for your radius client>
      ldap_suffix: <suffix of your root dn>
      ldap_root_user: <root user of your domain>
      ldap_root_secret: <the ldap root password>
